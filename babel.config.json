{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@service": "./src/service",
          "@scheduled-tasks": "./src/scheduled-tasks",
          "@utils": "./src/utils",
          "@api": "./src/api"
        }
      }
    ]
  ]
}
